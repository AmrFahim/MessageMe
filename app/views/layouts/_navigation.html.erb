<%= link_to "Chatroom" , root_path, class: "item"%>
<div class="right menu">
  <% if logged_in? %>
    <div class="item">
       <%= link_to "Log out" , logout_path, class: "ui primary button", method: :delete, data: "Are you sure"%>
    </div>
  <% else %>
    <div class="item">
       <%= link_to "Log in" , login_path, class: "ui primary button"%>
    </div>
    <div class="item">
       <%= link_to "Sign up" , new_user_path, class: "ui primary button"%>
    </div>
  <% end %>

</div>
